<tr data-id="<%= f.object.user.id %>" style="display: table-row">
  <td class="avatar-col">
    <%= f.hidden_field :user_id %>
    <%= f.hidden_field :role, value: User.roles[:supervisor] %>
    <%= image_tag f.object.user.avatar.url, class: "img-circle avatar-size" %>
  </td>
  <td class="fullname-col"><%= f.object.user.fullname %></td>
  <td class="email-col"><%= f.object.user.email %></td>
  <td class="birthday-col"><%= l f.object.user.birthday, format: :default %></td>
  <td class="gender-col">
    <i class="fa <%= display_gender f.object.user %>"></i>
  </td>
  <td class="option">
    <%= f.hidden_field :_destroy %>
    <%= link_to fa_icon("trash", text: ""), nil,
      class: "remove_record remove-position" %>
  </td>
</tr>
